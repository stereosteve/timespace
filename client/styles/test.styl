*
  margin 0
  padding 0

body
  color #555

.Timeline
  position relative
  background #f7f7f7
  height 2500px

  .Axis
    //position relative
    height 100%
    width: 100px
    background: #ccc
    border-right 1px solid #333

    .AxisLabel
      position absolute
      width: 100px
      text-align: center
      font-weight bold

  .TimePoint
    position absolute
    top 150px
    border-top: 1px solid #333
    margin-left: 100px
    padding: 10px
